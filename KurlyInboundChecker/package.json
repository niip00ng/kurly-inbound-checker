{
  "name": "KurlyInboundChecker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "react-native start --reset-cache",
    "android": "react-native run-android",
    "build-android-dev": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -rf android/app/src/main/res/drawable-* && cd android && ./gradlew clean && ENVFILE=.env.dev ./gradlew assembleRelease",
    "build-android-prod": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -rf android/app/src/main/res/drawable-* && cd android && ./gradlew clean && ENVFILE=.env.prod ./gradlew assembleRelease",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --udid 1E675441-F475-4B1F-B6D3-020E39D209DE",
    "ios": "ENVFILE=.env.dev react-native run-ios --udid 1E675441-F475-4B1F-B6D3-020E39D209DE",
    "ios15": "react-native run-ios --udid 4DF05B69-2DFD-4DF1-8F82-3CADE429D3AC",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "device-list-ios": "xcrun simctl list devices",
    "watchman-clean": "watchman watch-del-all & watchman shutdown-server",
    "device-android": "adb devices & adb pair ip:port"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.1.3",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/native-stack": "^7.1.14",
    "@reduxjs/toolkit": "^2.5.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-device-info": "^14.0.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.3.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
